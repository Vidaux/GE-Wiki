.WPC .WPC-proto {
	display: table;
	position: fixed;
	width:395px;
}

.WPC .WPC-proto-title {
	cursor: move;
	padding: 4px 23px 3px 5px;
	position: relative;
	width: 66%;
	border-width: 2px 1px 0 2px;
	-moz-border-radius-topright: 6px;
	-webkit-border-top-right-radius: 6px;
}

.WPC .WPC-proto .WPC-close {
	position:relative;
	margin-right: -17px;
}

.WPC .WPC-proto-control-grab {
	width: 12px;
	height: 41px;
	position: absolute;
	top: -2px;
	left: -12px;
	margin-left: 0px;
	-moz-border-radius-topleft: 6px;
	-webkit-border-top-left-radius: 6px;
	cursor: move;
	border-width: 2px 0 2px 1px;
}

.WPC .WPC-proto .WPC-proto-content-container {
	margin-top: -1px;
	padding: 3px;
	width: auto !important;
	border-width: 1px 1px 2px 2px;
	-moz-border-radius: 0 6px 6px;
	-webkit-border-radius: 6px;
	-webkit-border-top-left-radius: 0;
	-moz-box-shadow: 2px 4px 7px #333;
	-webkit-box-shadow: 2px 4px 7px #333;
}

.WPC .WPC-proto .WPC-proto-content-container .WPC-proto-content {
	padding: 10px 8px 8px 8px;
	height:auto;
	-moz-border-radius: 3px;
	-webkit-border-radius: 3px;
	
	_width:100%;
}


.WPC .WPC-proto-tabs {
	margin:0 10px 1em 0;
	text-align:right;
	white-space:nowrap;
}
.WPC .WPC-proto-tabs .WPC-selected  {
	font-weight:bold;
}
.WPC .WPC-lightbox .WPC-proto-tabs {
	text-align:left;
}


.WPC .WPC-proto-subWindow {
	position:relative;
	overflow:auto;
	border:3px solid;
	margin:5px 0;
	height:30em;
	width:37em;
}


.WPC .WPC-proto-subWindow {
	border-color:#666666 #999999 #CCCCCC #DDDDDD;
	/*
	background-color:#FFFFFF;
	color:#333333;
	*/
}


.WPC .WPC-proto-title {
