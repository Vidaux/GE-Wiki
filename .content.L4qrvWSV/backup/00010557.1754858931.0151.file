}

 
.WPC td.WPC-toggleHolder, 
.WPC-inherited td.WPC-toggleHolder{
 	width:15px;
 	padding:4px 0 4px 4px
 }
 
.WPC a.WPC-expandToggle,
.WPC-inherited a.WPC-expandToggle {
	display:block;
	line-height:15px;
	height:15px;
	width:15px;
	margin-top:2px;
	text-decoration:none;
	background-color: transparent;
	background-image: url(/web/20160111172823im_/http://static.wikifoundry.com/img/bg/bg_arrows.png?v=rel-55-44-0);
	background-repeat: no-repeat;
}
.WPC a.WPC-expandToggle:link:hover,
.WPC a.WPC-expandToggle:visited:hover,
.WPC-inherited a.WPC-expandToggle:link:hover,
.WPC-inherited a.WPC-expandToggle:visited:hover  {
	text-decoration:none;
}
.WPC a.WPC-expandToggleExpanded,
.WPC-inherited a.WPC-expandToggleExpanded {
	background-position: 50% 3px;
}
* html .WPC-inherited a.WPC-expandToggleExpanded {
	background-position: -12% 5px;
}
	
.WPC a.WPC-expandToggleCollapsed,
.WPC-inherited a.WPC-expandToggleCollapsed {
	background-position: 3px 50%;
}




