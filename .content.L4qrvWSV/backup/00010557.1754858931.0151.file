

#A { display: none; }

.WPC div, .WPC span, .WPC h1, .WPC h2, .WPC h3, .WPC h4, .WPC h5, .WPC h6, .WPC p, .WPC a, .WPC i, .WPC em, .WPC img, .WPC b, .WPC strong, 
.WPC ol, .WPC ul, .WPC li, .WPC form, .WPC label, .WPC table, .WPC tbody, .WPC tfoot, .WPC thead, .WPC input, .WPC textarea,
.WPC select, .WPC tr, .WPC th, .WPC td {
	outline: 0;
	border: 0 none;
	font-weight: inherit;
	font-style: inherit;
	font-size: 100%;
	font-family: inherit;
	color:inherit;
	letter-spacing:normal;
	text-decoration:none;	
	vertical-align: baseline;
	text-align: inherit;
	word-spacing:inherit;
	background: transparent none;
	margin: 0;
	padding: 0;
	position:static;
	line-height: inherit;	
	
	*line-height: normal;	
}
.WPC i, .WPC em{
	font-style:italic;
}
.WPC b, .WPC strong{
	font-weight:bold;
}




.WPC {
	font-family: Arial, Helvetica, sans-serif;
	font-size: 13px;
	text-align: left;
}
.WPC {
	color:#333333;
}





#WPC-controlElement {
	z-index: 1000;
	position: absolute;
	top: 0px;
	left: 0px;
}

.WPC #WPC-overlay {
	position:absolute;
	top:0px;
	left:0px;
	display:block;
	filter:alpha(opacity=50);
	opacity:.50;
	background-color:#EEEEEE;
}

.WPC .WPC-layers {
	position: absolute;
	top: 0px;
	left: 0px;
}

.WPC .WPC-layers .WPC-layer {
	position: absolute;
}
.WPC .WPC-outline {
	border-style:outset;
	border-width:1px;
	position:absolute;
	z-index:20;
}


.WPC-fieldData {
	display: none;
	overflow: hidden;
	width: 0px;
}
.WPC-fieldLoading {
	display: block;
	overflow: visible;
}


.WPC .WPC-throbberActive, 
.WPC .WPC-dialog .WPC-throbberActive, 
.WPC .WPC-dialog input.WPC-throbberActive, .WPC .WPC-dialog button.WPC-throbberActive
 {
	background-image:url(/web/20160111172823im_/http://static.wikifoundry.com/img/throbber.gif?v=rel-55-44-0);
	background-repeat:no-repeat;
	background-position:right;
}

* html .WPC .WPC-throbberActive, 
* html .WPC input.WPC-throbberActive,
* html .WPC button.WPC-throbberActive {
	background-image:url(/web/20160111172823im_/http://static.wikifoundry.com/img/throbber.gif?v=rel-55-44-0);
}
.WPC .WPC-formButtons input.WPC-throbberActive {
	padding-right:20px;
}
.WPC .WPC-dialog .WPC-throbberInactive {
	background-image:none;
	background-color:transparent;
}


.WPC-close,
.WPC .WPC-close {
	cursor: pointer;
	float: right;
	height: 16px;
	position: relative;
	width: 16px;
	background-repeat: no-repeat;
	background-image: url(/web/20160111172823im_/http://static.wikifoundry.com/img/btn/btn_close.png?v=rel-55-44-0);
}
* html .WPC-close,
* html .WPC .WPC-close {
	background-image: none;
	filter:progid:DXImageTransform.Microsoft.AlphaImageLoader(src='/img/btn/btn_close.png?v=rel-55-44-0', sizingMethod='crop');
}


.WPC-moreHelp,
.WPC .WPC-moreHelp {
	background: url(/web/20160111172823im_/http://static.wikifoundry.com/img/icon/icon_moreHelp.png?v=rel-55-44-0) 2px 0.3em no-repeat;
	padding-left: 15px;
}
.WPC-title, .WPC .WPC-title {
	font-weight:bold;
}

.WPC .WPC-altBg, .WPC tr.WPC-altBg td {
