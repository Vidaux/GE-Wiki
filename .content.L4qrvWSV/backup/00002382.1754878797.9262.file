                  <b><span class="diffFormatting" title="">Status</span></b><span class="diffFormatting" title="">:</span><br /></font> <span class="diffFormatting" title=""></span> <span class="diffFormatting" title=""></span> <span class="diffFormatting" title=""></span> <span class="diffFormatting" title=""></span> <span class="diffFormatting" title=""></span> <span class="diffFormatting" title=""></span> <span class="diffFormatting" title=""></span> <span class="diffFormatting" title=""></span> <span class="diffFormatting" title=""></span> <span class="diffFormatting" title=""></span> <span class="diffFormatting" title=""></span> <span class="diffFormatting" title=""></span>
                  <table align="bottom" cellpadding="3" class="wp-border-all" width="400">
                    <tbody>
                      <tr>
                        <td align="center" width="16%"><font size="2"><b><font color="#FF0000">Str</font></b></font></td>
                        <td align="center" width="16%"><span class="diffFormatting" title=""></span><font size="2"><b><font color="#FF0000">Agi</font></b></font></td>
                        <td align="center" width="16%"><span class="diffFormatting" title=""></span><font size="2"><b><font color="#FF0000">Con</font></b></font></td>
                        <td align="center" width="16%"><span class="diffFormatting" title=""></span><font size="2"><b><font color="#FF0000">Dex</font></b></font></td>
                        <td align="center" width="16%"><span class="diffFormatting" title=""></span><font size="2"><b><font color="#FF0000">Int</font></b></font></td>
